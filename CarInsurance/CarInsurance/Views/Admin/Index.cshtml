@using CarInsurance.ViewModels
@model List<InsureeUser>


<table class="table table-striped">
    <thead class="thead-dark">
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email Address</th>
            <th>Monthly Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.EmailAddress</td>
                <td>@user.Quote</td>
            </tr>
        }
    </tbody>
</table>

